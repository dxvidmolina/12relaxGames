<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/styleListado.css">
    <title>Document</title>
</head>
<body>
    <div>
        <%- include("./partials/header") %>
    </div>

    <section class="articulos">
        <% producto.forEach(function(producto){ %>
            <article>
                <div class="imagen">
                    <img src="/productos/Producto_img/<%=producto.img%>" alt="">
                </div>
               <div class="parametrosFlex">
                <div class="parametros">
                    <ul>
                        <li>Nombre: <%= producto.nombreJuego %>  </li>
                        <li>Descuento: <%=producto.descuento  %>  </li>
                        <li>Precio: $ <%=producto.precio %>  </li>
                    </ul>
                </div>
               </div>
            </article>
        <% }) %>
    </section>

<div>
    <%- include("./partials/footer") %>
</div>
</body>
</html>